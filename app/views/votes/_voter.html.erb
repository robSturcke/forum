<% if current_user %>
<div class="pull-left vote-arrows">
  <div> <%= link_to " ", post_up_vote_path(post), class: 'icon ion-ios-pint', method: :post %> </div>
  <div> <strong> <%= post.points %> </strong> </div>
  <div> <%= link_to " ", post_down_vote_path(post), class: 'icon ion-ios-pint-outline', method: :post %> </div>
</div>
<% end %>
